{
    "version": 2,
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ],
    "builds": [

        {
            "src": "./bin/server.js",
            "use": "@vercel/node"

        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./bin/server.js"
        }
    ]
}